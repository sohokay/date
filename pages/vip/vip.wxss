page {
    background-color: #f6f6f6;
}

.vip-wrap {
    background: linear-gradient(#fc4529,#ff7b43);
    border-radius: 0 0 50% 50%/50% 50% 40rpx 40rpx;
    height: 350rpx;
    overflow: hidden;
    padding: 0 32rpx;
    width: calc(100% - 64rpx);
}

.card-wrap {
    height: 400rpx;
    position: relative;
}

.vip-discount {
    font-size: 28rpx;
    height: 140rpx;
    line-height: 40rpx;
    right: 32rpx;
    top: 0;
    width: 80rpx;
}

.vip-discount,.vip-price {
    color: #fff;
    font-weight: 400;
    padding: 0;
    position: absolute;
    text-align: center;
}

.vip-price {
    bottom: 90rpx;
    font-size: 30rpx;
    height: 72rpx;
    left: -12rpx;
    line-height: 68rpx;
    width: 254rpx;
}

.card-uid {
    color: #fff;
    font-size: 28rpx;
    font-weight: 500;
}

.card-vip {
    color: hsla(0,0%,100%,.7);
    font-size: 24rpx;
    font-weight: 400;
    margin-top: 10rpx;
}

.vipbtn-wrap {
    align-items: center;
    bottom: 90rpx;
    display: flex;
    flex-direction: row;
    height: 72rpx;
    justify-content: center;
    position: absolute;
    right: 32rpx;
}

.buy-btn {
    border: none;
    color: #fff;
    font-size: 28rpx;
    font-weight: 400;
    height: 72rpx;
    line-height: 72rpx;
    padding: 0 25rpx 0 20rpx;
    text-align: center;
}

.vipbtn-wrap::after {
    border-color: #fff;
    border-style: solid;
    border-width: 4rpx 4rpx 0 0;
    content: " ";
    display: inline-block;
    height: 12rpx;
    position: absolute;
    right: 4rpx;
    top: 50%;
    transform: translateY(-50%) matrix(.71,.71,-.71,.71,0,0);
    width: 12rpx;
}

.vip-content {
    background-color: #fff;
    border-radius: 12rpx;
    box-shadow: 0 4rpx 40rpx 0 rgba(231,200,217,.2);
    margin-top: 32rpx;
    width: 100%;
}

.vip-content-item {
    align-items: center;
    border-bottom: 1rpx solid #eee;
    border-right: 1rpx solid #eee;
    display: flex;
    flex-direction: column;
    justify-content: center;
    min-height: 180rpx;
    overflow: hidden;
    padding: 30rpx;
    position: relative;
}

.content-current {
    background-color: #999;
    color: #fff;
    font-size: 18rpx;
    height: 70rpx;
    left: -70rpx;
    line-height: 90rpx;
    padding-top: 70rpx;
    position: absolute;
    text-align: center;
    top: -70rpx;
    transform: rotate(-45deg);
    width: 140rpx;
}

.vip-content-item image {
    height: 64rpx;
    width: 64rpx;
}

.vip-content-item .content-text {
    color: #333;
    font-size: 28rpx;
    font-weight: 400;
    padding: 15rpx 0;
    text-align: center;
}

.content-text image {
    height: 28rpx;
    margin-left: 4rpx;
    vertical-align: middle;
    width: 28rpx;
}

.vip-renewal {
    bottom: 90rpx;
    color: #fff;
    font-size: 24rpx;
    font-weight: 400;
    height: 72rpx;
    line-height: 72rpx;
    padding: 0 20rpx;
    position: absolute;
    right: 32rpx;
    text-align: center;
}

.vip-discount-bg {
    background-image: url("data:image/png;base64,");
}

.vip-discount-bg,.vip-price-bg {
    background-repeat: no-repeat;
    background-size: contain;
}

.vip-price-bg {
    background-image: url("data:image/png;base64,");
}

.card-wrap-img {
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: contain;
}

.vip-popup {
    align-items: center;
    background-color: rgba(0,0,0,.5);
    display: flex;
    flex-direction: column;
    height: 100vh;
    justify-content: center;
    position: fixed;
    top: 0;
    width: 100vw;
    z-index: 1000;
}

.vip-popup-wrap {
    background-color: #fff;
    border-radius: 12rpx;
    padding: 40rpx 32rpx;
    width: 75vw;
}

.vip-popup-wrap .title {
    color: #333;
    font-size: 32rpx;
    font-weight: 700;
    line-height: 44rpx;
}

.vip-popup-wrap .content {
    color: #333;
    padding: 20rpx 0;
}

.cancel-btn,.vip-popup-wrap .content {
    font-size: 28rpx;
    font-weight: 400;
    line-height: 40rpx;
}

.cancel-btn {
    color: #fc4529;
    float: right;
}

.userImg {
    border-radius: 50%;
    height: 120rpx;
    width: 120rpx;
}