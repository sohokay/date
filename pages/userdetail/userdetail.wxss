page {
    background-color: #f4f4f4;
    padding-bottom: 140rpx;
}

.swiper-wrap {
    background-color: #f6f6f6;
    height: 750rpx;
    width: 750rpx;
}

.swiper_item {
    height: 100%;
    position: relative;
    width: 100%;
}

.swiper-count {
    background-color: rgba(0,0,0,.4);
    border-radius: 24rpx;
    bottom: 30rpx;
    color: #fff;
    font-size: 20rpx;
    line-height: 20rpx;
    padding: 6rpx 20rpx;
    position: absolute;
    right: 30rpx;
}

.swiper-image {
    display: block;
    height: 100%;
    margin: 0;
    width: 100%;
}

.userListBox {
    background-color: #fff;
    padding: 20rpx;
    width: 710rpx;
}

.userListc {
    padding: 20rpx 0;
    position: relative;
    width: 710rpx;
}

.userimg {
    border-radius: 80rpx;
    margin-right: 30rpx;
    width: 160rpx;
}

.h_160,.userimg {
    height: 160rpx;
}

.sexIcon {
    height: 30rpx;
    width: 30rpx;
}

.shimingIcon {
    height: 30rpx;
    max-width: 80rpx;
}

.shuxian {
    margin: 0 15rpx;
}

.lianxi {
    border: 1rpx solid #fe9e93;
    border-radius: 25rpx;
    color: #fb250d;
    font-size: 22rpx;
    font-weight: 500;
    height: 50rpx;
    line-height: 50rpx;
    position: absolute;
    right: 0rpx;
    text-align: center;
    top: 90rpx;
    width: 115rpx;
}

.title {
    color: #333;
    font-size: 32rpx;
    font-weight: 600;
    line-height: 44rpx;
    margin-bottom: 10rpx;
    position: relative;
}

.title::after {
    background: linear-gradient(90deg,#fc4529,rgba(255,123,67,.5));
    border-radius: 4rpx;
    bottom: 2rpx;
    content: "";
    height: 8rpx;
    left: 0;
    position: absolute;
    width: 80rpx;
}

.infoBox {
    background: #fff;
    border-radius: 10rpx;
    padding: 40rpx;
    width: 600rpx;
}

.paTB_10 {
    padding: 10rpx 0;
}

.label-item {
    background-color: #e8d3ba;
    border-radius: 64rpx;
    color: #333;
    display: inline-block;
    font-size: 24rpx;
    margin: 5rpx;
    padding: 5rpx 30rpx;
}

.like-wrap {
    background: #fff;
    border-radius: 80rpx;
    box-shadow: 0 4rpx 32rpx 0 rgba(231,200,217,.4);
    height: 80rpx;
    position: absolute;
    right: 20rpx;
    top: 60rpx;
    width: 80rpx;
}

.like-wrap image {
    height: 48rpx;
    margin: 16rpx;
    width: 48rpx;
}

.foot-btn-bar {
    align-items: center;
    background-color: #fff;
    bottom: 0;
    box-shadow: 0 4rpx 40rpx 0 rgba(231,200,217,.2);
    display: flex;
    flex-direction: row;
    height: 100rpx;
    justify-content: space-around;
    position: fixed;
    width: 100%;
}

.contact-btn {
    background: linear-gradient(90deg,#fa270c,#fa3e0c,#fb580d);
    border-radius: 40rpx;
    color: #fff;
    font-size: 32rpx;
    font-weight: 400;
    height: 80rpx;
    line-height: 80rpx;
    padding: 0 30rpx;
    width: calc(60% - 60rpx);
}

.image-btn-list {
    align-content: center;
    flex-direction: row;
    justify-content: space-around;
    width: 40%;
}

.image-btn,.image-btn-list {
    display: flex;
}

.image-btn {
    align-items: center;
    color: #999;
    flex-direction: column;
    font-size: 20rpx;
    font-weight: 400;
    justify-content: center;
}

.btn-image {
    height: 48rpx;
    padding: 0 10rpx;
    width: 48rpx;
}

button.share-btn {
    -webkit-tap-highlight-color: transparent;
    align-items: center;
    background-color: #fff;
    border-radius: 0;
    box-sizing: border-box;
    color: #999;
    display: flex;
    flex-direction: column;
    font-size: 20rpx;
    font-weight: 400;
    justify-content: center;
    line-height: 34rpx;
    overflow: hidden;
    padding-left: 0;
    padding-right: 0;
    position: relative;
    text-align: center;
    text-decoration: none;
    width: 40rpx!important;
}

button::after {
    border: none;
}

.fu-wrap {
    align-items: center;
    bottom: 140rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    position: fixed;
    right: 30rpx;
    z-index: 1000;
}

.fu-wrap image {
    height: 120rpx;
    vertical-align: middle;
    width: 120rpx;
}

.fu-wrap view {
    color: #fff;
    font-size: 28rpx;
    text-shadow: 0 0 .2em #c82349,0 0 .2em #c82349;
}

.marB140 {
    margin-bottom: 140rpx;
}

.descriptionT {
    background: #f5f5f5;
    border: 30rpx solid #fff;
    font-size: 30rpx;
    padding: 20rpx;
    text-align: center;
    width: 650rpx;
}